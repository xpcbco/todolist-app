{
  "name": "todolist-app",
  "version": "1.0.0",
  "description": "TodoList App",
  "main": "index.ts",
  "scripts": {
    "test": "",
    "dev": "ts-node src",
    "tsc": "tsc",
    "tsc:init": "tsc --init",
    "knex": "knex --knexfile src/knexfile.ts",
    "knex:init": "knex init -x ts",
    "migrate:latest": "npm run knex migrate:latest",
    "migrate:list": "npm run knex migrate:list",
    "migrate:make": "npm run knex migrate:make",
    "migrate:up": "npm run knex migrate:up",
    "migrate:down": "npm run knex migrate:down",
    "migrate:rollback": "npm run knex migrate:rollback --all --debug",
    "seed:make": "npm run knex seed:make",
    "seed:run": "npm run knex seed:run --debug"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "apollo-server-express": "^3.6.2",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "knex": "^1.0.1",
    "knex-stringcase": "^1.4.6",
    "objection": "^3.0.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
